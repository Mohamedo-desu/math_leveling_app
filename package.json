{
  "name": "mental_math_training_app",
  "main": "App.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "build:android": "eas build --platform android --profile preview",
    "offline": "expo start --offline",
    "publish:expo": "eas update --branch preview",
    "build:web": "npx expo export --platform web",
    "release:web": "eas deploy --prod",
    "upgrade": "bun install expo@latest && bunx expo install --fix",
    "android": "bunx expo run:android --device --variant debug",
    "release:android": "bunx expo run:android --device --variant Release",
    "release:ios": "bunx expo run:ios --configuration Release",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "babel-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "expo": "~53.0.9",
    "expo-application": "~6.1.4",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-notifications": "~0.31.2",
    "expo-splash-screen": "~0.30.8",
    "expo-updates": "~0.28.13",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-animated-numbers": "^0.6.3",
    "react-native-mmkv": "^3.2.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-svg": "^15.12.0",
    "react-native-web": "~0.20.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eas-build-cache-provider": "^16.4.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
