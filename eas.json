{
  "cli": {
    "version": ">= 13.2.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "channel": "development"
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "internal",
      "environment": "preview",
      "channel": "preview",
      "android": { "image": "latest" }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "channel": "production",
      "android": { "image": "latest" }
    }
  },
  "submit": {
    "production": {}
  }
}
